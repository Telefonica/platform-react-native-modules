{
  "name": "@telefonica/ios-common-cookies",
  "version": "1.0.0",
  "description": "Expo iOS module for synchronizing cookies between HTTPCookieStorage and WKWebView",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Telefonica/platform-react-native-modules"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "ios",
    "expo-module.config.json",
    "README.md",
    "iOSCommonCookies.podspec"
  ],
  "dependencies": {
    "expo-modules-core": "^2.5.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.72"
  },
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts --clean",
    "ts:check": "tsc --noEmit --project tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  }
}